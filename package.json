{
  "name": "dodo-payments-supabase-boilerplate",
  "version": "1.0.0",
  "description": "A TypeScript boilerplate for integrating Dodo Payments with Supabase",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "start": "node dist/index.js",
