    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "crypto-js": "^4.2.0",
    "dodopayments": "^1.51.2",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "zod": "^3.22.4"
